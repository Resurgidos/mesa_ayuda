<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.EstadoCaso" table="estadocaso">

        <id name="OID" type="string">
            <column name="OIDEstadoCaso"/>
            <generator class="uuid2"/>
            
        </id>

        <property name="codEstadoCaso" type="int">
            <column name="codEstadoCaso"/>
        </property>

        <property name="nombreEstadoCaso" type="string">
            <column name="nombreEstadoCaso"/>
        </property>
        
        <property name="fechaHoraFinVigenciaEC" type="date">
            <column name="fechaHoraFinVigenciaEC"/>
        </property>
        
        
        
       
        

    </class>

</hibernate-mapping>