<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.CasoInstanciaTarea" table="casoinstanciatarea">

        <id name="OID" type="string">
            <column name="OIDCasoIntanciaTarea"/>
            <generator class="uuid2"/>
        
        </id>

        <property name="fechaHoraInicioCasoInstanciaTarea" type="date">
            <column name="fechaHoraInicioCasoInstanciaTarea"/>
        </property>

        <property name="fechaHoraFinCasoInstanciaTarea" type="date">
            <column name="fechaHoraFinCasoInstanciaTarea"/>
        </property>
        
        <many-to-one name="tipoTarea" class="entidades.TipoTarea" column="OIDTipoTarea" not-null="true"/>

    </class>

</hibernate-mapping>