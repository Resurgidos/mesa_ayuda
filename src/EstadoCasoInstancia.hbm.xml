<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.EstadoCasoInstancia" table="estadocasoinstancia">

        <id name="OID" type="string">
            <column name="OIDEstadoCasoInstancia"/>
            <generator class="uuid2"/>
        </id>

        <property name="codEstadoCasoInstancia" type="int">
            <column name="codEstadoCasoInstancia"/>
        </property>

        <property name="nombreEstadoCasoInstancia" type="string">
            <column name="nombreEstadoCasoInstancia"/>
        </property>
        
        <property name="fechaHoraFinVigenciaECI" type="date">
            <column name="fechaHoraFinVigenciaECI"/>
        </property>
        
        
        
       
        

    </class>

</hibernate-mapping>