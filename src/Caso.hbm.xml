<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entidades.Caso" table="caso">

        <id name="OID" type="string">
            <column name="OIDCaso"/>
            <generator class="uuid2"/>
      
        </id>

        <property name="numeroCaso" type="int">
            <column name="numeroCaso"/>
        </property>

        <property name="fechaHoraCaducidadCaso" type="date">
            <column name="fechaHoraCaducidadCaso"/>
        </property>
        
        <property name="fechaHoraFinCaso" type="date">
            <column name="fechaHoraFinCaso"/>
        </property>
        
        <property name="fechaHoraIngresoCaso" type="date">
            <column name="fechaHoraIngresoCaso"/>
        </property>
        
        <property name="observacionCaso" type="string">
            <column name="observacionesCaso"/>
        </property>
        
        <many-to-one name="estCaso" class="entidades.EstadoCaso" column="OIDEstadoCaso" not-null="true"/>
        <many-to-one name="tipoCaso" class="entidades.TipoCaso" column="OIDTipoCaso" not-null="true"/>
        
        <bag name="casoinstancia" table="casoinstancia" cascade="save-update" > 
            <key column="OIDCaso" not-null="true"/> 
            <one-to-many class="entidades.CasoInstancia"/> 
        </bag>
       
    </class>

</hibernate-mapping>
